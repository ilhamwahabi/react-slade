{
  "name": "react-slade",
  "version": "1.5.0",
  "description": "React dialog component that slide-able",
  "main": "dist/index.js",
  "author": "Ilham Wahabi",
  "homepage": "https://github.com/iwgx/react-slade",
  "repository": {
    "type": "git",
    "url": "https://github.com/iwgx/react-slade"
  },
  "license": "MIT",
  "files": [
    "dist",
    "lib",
    "src",
    "build"
  ],
  "scripts": {
    "lint:check": "eslint --ext .js .",
    "lint:fix": "eslint --ext .js . --fix",
    "prettier:check": "prettier --check '**/*.{js,css,md,json}'",
    "prettier:fix": "prettier --write '**/*.{js,css,md,json}'",
    "fix": "npm run lint:fix && npm run prettier:fix",
    "build:lib": "babel lib -d dist",
    "build:lib:watch": "babel lib -w -d dist",
    "prepare": "npm run build:lib",
    "example": "webpack-dev-server --open --mode development",
    "build:example": "webpack --mode=production",
    "predeploy": "npm run build:lib",
    "deploy": "gh-pages -d build"
  },
  "lint-staged": {
    "*.{js,css,md,json}": [
      "npm run fix",
      "git add"
    ]
  },
  "tags": [
    "react",
    "modal",
    "dialog",
    "slide"
  ],
  "keywords": [
    "react",
    "modal",
    "dialog",
    "slide",
    "react-component"
  ],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.6.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-react-css-modules": "^3.4.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^1.0.0",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-watch": "^4.0.2",
    "file-loader": "^1.1.11",
    "gh-pages": "^1.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.3.1",
    "jest": "^23.6.0",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2",
    "prop-types": "^15.6.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-test-renderer": "^16.5.1",
    "regenerator-runtime": "^0.12.1",
    "style-loader": "^0.21.0",
    "webpack": "^4.16.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  },
  "dependencies": {
    "styled-components": "^3.3.3"
  }
}
